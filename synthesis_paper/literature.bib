	Issn = {1944-7973},
	Journal = wrr,
	Keywords = {amr; patch-based; porous media; cut cells},
	Number = {3},
	Title = {{An investigation of the effect of pore scale flow on average geochemical reaction rates using direct numerical simulation}},
	Url = {http://dx.doi.org/10.1029/2011WR011404},
