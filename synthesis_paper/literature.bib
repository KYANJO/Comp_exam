	Issn = {0021-9991},
	Journal = jcp,
	Keywords = {ppm; tracer; atmosphere; Semi-Lagrangian},
	Number = {4},
	Pages = {1215 - 1237},
	Title = {{A flux-form version of the conservative semi-Lagrangian multi-tracer transport scheme (CSLAM) on the cubed sphere grid}},
	Url = {http://www.sciencedirect.com/science/article/pii/S0021999110006005},
	Volume = {230},
	Year = {2011},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0021999110006005},
