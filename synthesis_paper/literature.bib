	Isbn = {2045-2322},
	Journal = {Scientific Reports},
	Keywords = {volcano},
	Number = {1},
	Pages = {12238},
	Title = {{New Zealand supereruption provides time marker for the Last Glacial Maximum in Antarctica}},
	Ty = {JOUR},
