	Issn = {1364-8152},
	Journal = envmodsoft,
	Keywords = {GPU; SWE; parallel; software; octree},
	Pages = {119 - 137},
	Title = {{A non-uniform efficient grid type for GPU-parallel Shallow Water Equations models}},
